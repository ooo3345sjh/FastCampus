package ch06;

import ch04.*;

interface iQueue {
	public void enQueue(String data);
	public String deQueue();
	public void printAll();
}

public class MyListQueue extends MyLinkedList implements iQueue{
	
	private MyListNode front;
	private MyListNode rear;
	
	public MyListQueue() {
		front = null;
		rear = null;
	}

	
	@Override
	public void enQueue(String data) {
		MyListNode newNode = addElement(data);
		if(isEmpty()) {
			front = newNode;
			rear = newNode;
		} else {
			rear = newNode;
		}
	}
	
	@Override
	public String deQueue() {
		if(isEmpty()) {
			System.out.println("Queue is empty");
			return null;
		} else {
			String data = front.getData();
			removeElement(0);
			if(getSize() == 0) {
				front = null;
				rear = null;
			}
			return data;
		}
	}
}
