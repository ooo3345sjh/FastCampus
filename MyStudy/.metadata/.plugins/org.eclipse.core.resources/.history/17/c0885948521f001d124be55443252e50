package ch03;

public class MyarrayTest {

	public static void main(String[] args) {
		MyArray mArr = new MyArray();
		mArr.addElement(1);
		mArr.addElement(2);
		mArr.addElement(3);
		mArr.addElement(4);
		mArr.insertElement(3, 15);
		mArr.printAll();
		
		System.out.println();
		System.out.println(mArr.getSize());
		
	}

}
