package ch14;

import java.io.*;

public class Test {
	public static void main(String[] args) throws IOException {
		Reader r = new FileReader("C:/Temp/test8.txt");
		BufferedReader br = new BufferedReader(r);
			
		while(true) {
			String data = br.readLine();
			if(data == null) break;
			System.out.println(data);
		}
		br.close()
	}
}
