package ch06;

import ch04.*;

interface iQueue {
	public void enQeueu(String data);
	public String deQeuue();
	public void printAll();
}

public class MyListQueue extends MyLinkedList implements iQueue{
	
	MyLinkedList list;
	MyListNode front;
	MyListNode rear;
	
	public MyListQueue() {
		list = new MyLinkedList();
		front = null;
		rear = null;
	}

	
	@Override
	public void enQueue(String data) {
		list.addElement(data);
		front = list.getNode(list.getSize()-1);
		rear = list.getNode(0);
	}
	
	@Override
	public String deQueue() {
		if(list.isEmpty()) {
			System.out.println("Queue is empty");
			return null;
		} else {
			list.removeElement(0);
			return rear.getData();
		}
	}
	
	@Override
	public void printAll() {
		list.printAll();
	}

}
