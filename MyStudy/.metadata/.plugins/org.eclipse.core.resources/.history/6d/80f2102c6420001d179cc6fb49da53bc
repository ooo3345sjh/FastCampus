package ch06;

import ch04.*;

public interface iQueue {
	void enQeueu(String data);
	String deQeuue();
	void printAll();
}

public class MyListQueue extends MyLinkedList implements{
	
	MyLinkedList list;
	MyListNode front;
	MyListNode rear;
	
	public MyListQueue() {
		list = new MyLinkedList();
		front = null;
		rear = null;
	}

	public void enQueue(String data) {
		list.addElement(data);
		front = list.getNode(list.getSize()-1);
		rear = list.getNode(0);
	}
	
	public String deQueue() {
		if(list.isEmpty()) {
			System.out.println("Queue is empty");
			return null;
		} else {
			list.removeElement(0);
			return rear.getData();
		}
	}
	
	public void printAll() {
		list.printAll();
	}
}
