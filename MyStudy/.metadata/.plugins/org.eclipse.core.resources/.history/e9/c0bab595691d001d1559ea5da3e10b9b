package ch06;

public class VIPCustomer extends Customer{
	private int agentID;
	double salesRatio;
	
	public VIPCustomer(int customerID, String customerName) {
		super(customerID, customerName);
		customerGrade = "VIP";    
		salesRatio = 0.1;
		bonusRatio = 0.05;
	}
	
	public int getAgentID() {
		return agentID;
	}
}
