package ch14;

public class Subway {
	private int subwayID;
	private int earnings;
	private int numberOfPassengers;
	
	public Subway(String color) {
		this.color = color;
		this.
		earnings = 0;
	}
	
	public void earningsSubway() {
		earnings += 1000;
		numberOfPassengers++;
	}
	
	public void showInfo() {
		System.out.println(color + "지하철의 승객은" + numberOfPassengers + " 이고, 수입은 " + earnings + "원 입니다.");
	}
	
}
