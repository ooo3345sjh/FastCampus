 * 내용 : 내가 구현한 ArrayList로 Stack 구현 해보기
 */

import ch03.*;

public class MyArrayStack {
	
	private MyArray array;
	private int count;
	
	public MyArrayStack() {
		array = new MyArray();
	}
	
	public MyArrayStack(int size) {
		array = new MyArray(size);
	}
	
	public void push(int num) {
		count++;
		array.addElement(num);
	}
	
	public int pop() {
		int popElement = array.removeElement(0);
		return popElement;
	}
	
	public int Peek() {
		int peekElement = array.getElement(0);
		return peekElement;
	}
	
	public int getSize() {
		return array.getSize();
	}
	
	public boolean isFull() {
		is(array.getSize())
	}
	
	public boolean isEmpty() {
		
	}
	
	public void printAll() {
		
	}
	
	

}
